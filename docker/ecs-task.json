{
  "containerDefinitions": [
    {
      "name": "morphrus-service",
      "image": "665378081513.dkr.ecr.ap-northeast-1.amazonaws.com/morpheus-repo:latest",
      "cpu": 0,
      "portMappings": [
        {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
        }
      ],
      "essential": true,
      "entryPoint": [],
      "command": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/morphrus-serivce",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "morphrus-serivce",
  "executionRoleArn": "arn:aws:iam::665378081513:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
